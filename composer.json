{
	"name": "magic3w/phpauth-sdk-php",
	"description": "PHP SDK for PHPAuthServer (magic3w/phpauth)",
	"type": "library",
	"license": "MIT",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"authors": [
		{
			"name": "CÃ©sar de la Cal Bretschneider",
			"email": "cesar@magic3w.com"
		}
	],

	"autoload": {
		"psr-4": {
			"magic3w\\phpauth\\sdk\\": "./src/"
		}
	},
	
	"scripts": {
